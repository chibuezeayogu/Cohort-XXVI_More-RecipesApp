{
  "name": "more-recipe",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting recipe ideas they have invented or learnt.",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=es2015",
    "start": "nodemon --exec npm run babel-node -- ./app.js",
    "build": "babel src -d dist",
    "test:db:migrate": "cross-env NODE_ENV=test sequelize db:migrate:undo:all && cross-env NODE_ENV=test sequelize db:migrate",
    "test:db:seed": "cross-env NODE_ENV=test sequelize db:seed:all",
    "start:test:db": "npm run test:db:migrate && npm run test:db:seed",
    "test": "npm run start:test:db && cross-env NODE_ENV=test nyc mocha ./test/apiTest/*.js --timeout 20000",
    "test-b": "SET NODE_ENV=test && nyc --reporter=html mocha --compilers js:babel-core/register ./test/apiTest/user.spec.js --timeout 20000",
    "migrate-test": "cross-env NODE_ENV=test sequelize db:migrate"
  },
  "author": "Chibueze Ayogu",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "^6.8.0",
