    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.8.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chi": "0.0.10",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.15.4",
